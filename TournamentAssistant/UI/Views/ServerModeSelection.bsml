<vertical child-control-height='false'
          xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
          xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
  <horizontal bg='panel-top' pad-left='10' pad-right='10' horizontal-fit='PreferredSize'>
    <text text='比赛助手' align='Center' font-size='10'></text>
  </horizontal>
  <horizontal bg='round-rect-panel' pad='8'>
    <text id='status-text' text='一起来玩呀' align='Center'></text>
  </horizontal>
  <horizontal>
    <button text='竞技比赛' id='tournament-button' on-click='tournament-button-pressed' hover-hint='在比赛日加入比赛大厅' />
    <button text='预选赛' id='qualifier-button' on-click='qualifier-button-pressed' hover-hint='进行预选赛曲目分数提交' />
    <button text='一起击剑' id='battlesaber-button' on-click='battlesaber-button-pressed' hover-hint='一起激情击剑🤺'/>
  </horizontal>
</vertical>